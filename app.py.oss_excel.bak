        async def handle_temp_room_owner_leave(request_group_id: Optional[str]):
            """æˆ¿é—´åˆ›å»ºè€…ä¸»åŠ¨è§£æ•£ä¸´æ—¶æˆ¿é—´"""
            group_key = (request_group_id or "").strip()
