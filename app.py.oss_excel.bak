                error_response = {
                    "type": "temp_room_owner_leave",
                    "status": "error",
                    "message": "group_id ä¸èƒ½ä¸ºç©º"
                }
                await websocket.send_text(json.dumps(error_response, ensure_ascii=False))
                return

            room_info = active_temp_rooms.get(group_key)
            if not room_info:
                error_response = {
                    "type": "temp_room_owner_leave",
                    "status": "not_found",
                    "group_id": group_key,
                    "message": "æœªæ‰¾åˆ°ä¸´æ—¶æˆ¿é—´æˆ–å·²è§£æ•£"
                }
                await websocket.send_text(json.dumps(error_response, ensure_ascii=False))
