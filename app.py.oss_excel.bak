async def heartbeat_checker():
    try:
        while not stop_event.is_set():
            now = time.time()
            to_remove = []
            for uid, conn in list(connections.items()):
                if now - conn["last_heartbeat"] > 30:
                    print(f"ç”¨æˆ· {uid} å¿ƒè·³è¶…æ—¶ï¼Œæ–­å¼€è¿æ¥")
                    await safe_close(conn["ws"], 1001, "Heartbeat timeout")
                    to_remove.append(uid)
            for uid in to_remove:
                connections.pop(uid, None)  # å®‰å…¨ç§»é™¤
                # æ¸…ç†ç”¨æˆ·ä»æ‰€æœ‰ä¸´æ—¶æˆ¿é—´çš„æˆå‘˜åˆ—è¡¨ä¸­ç§»é™¤
                # æ³¨æ„ï¼šä¸å†å› ä¸ºå¿ƒè·³è¶…æ—¶è‡ªåŠ¨è§£æ•£æˆ¿é—´ï¼Œåªç§»é™¤æˆå‘˜ï¼Œæˆ¿é—´æ˜¯å¦è§£æ•£ç”±ä¸šåŠ¡æ¶ˆæ¯æ§åˆ¶ï¼ˆå¦‚ temp_room_owner_leaveï¼‰
                for group_id, room_info in list(active_temp_rooms.items()):
                    members = room_info.get("members", [])
                    if uid in members:
                        members.remove(uid)
                        print(f"[webrtc] å¿ƒè·³è¶…æ—¶ï¼šç”¨æˆ· {uid} ç¦»å¼€æˆ¿é—´ {group_id}ï¼Œå½“å‰æˆå‘˜æ•°={len(members)}")
            await asyncio.sleep(10)
    except asyncio.CancelledError:
        print("heartbeat_checker å·²å®‰å…¨é€€å‡º")

from contextlib import asynccontextmanager
# ===== ç”Ÿå‘½å‘¨æœŸç®¡ç† =====
@asynccontextmanager
async def lifespan(app: FastAPI):
    global stop_event
    stop_event.clear()

    # å¯åŠ¨æ—¶ä»æ•°æ®åº“åŠ è½½ä»ç„¶æ´»è·ƒçš„ä¸´æ—¶è¯­éŸ³æˆ¿é—´
    load_active_temp_rooms_from_db()

    # å¯åŠ¨å¿ƒè·³æ£€æµ‹ä»»åŠ¡
    hb_task = asyncio.create_task(heartbeat_checker())
    print("ğŸš€ åº”ç”¨å¯åŠ¨ï¼Œå¿ƒè·³æ£€æµ‹å·²å¯åŠ¨")

    yield  # åº”ç”¨è¿è¡Œä¸­

    # åº”ç”¨å…³é—­é€»è¾‘
    print("ğŸ›‘ åº”ç”¨å…³é—­ï¼Œå‡†å¤‡åœæ­¢å¿ƒè·³æ£€æµ‹")
    stop_event.set()  # é€šçŸ¥å¿ƒè·³é€€å‡º
    hb_task.cancel()  # å¼ºåˆ¶å–æ¶ˆ
    try:
        await hb_task
    except asyncio.CancelledError:
        print("heartbeat_checker å·²å®‰å…¨åœæ‰")

app = FastAPI(lifespan=lifespan)

