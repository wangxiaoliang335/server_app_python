        return JSONResponse({'data': {'message': 'è·å–ç”¨æˆ·ä¿¡æ¯å¤±è´¥', 'code': 500, 'userinfo': []}}, status_code=500)
    except Exception as e:
        print("Unexpected error during fetching userinfo:", e)
